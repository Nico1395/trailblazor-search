<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup>
		<EnablePackageValidation>true</EnablePackageValidation>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<PackageId>Trailblazor.Search</PackageId>
		<Authors>Nicolai Wolf</Authors>
		<Description></Description>
		<RepositoryUrl>https://github.com/Nico1395/trailblazor-search</RepositoryUrl>
		<PackageTags>Blazor;Search;Architecture</PackageTags>
		<PackageLicenseExpression></PackageLicenseExpression>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<!-- TODO -> README.md -->
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.0" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.0" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Pipelining\" />
	</ItemGroup>

</Project>
